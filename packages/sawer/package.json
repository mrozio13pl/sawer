{
    "name": "sawer",
    "version": "0.0.1",
    "type": "module",
    "description": "A minimalistic, pure HTTP framework for backend applications",
    "homepage": "https://github.com/mrozio13pl/sawer/blob/main/packages/sawer",
    "repository": "mrozio13pl/sawer",
    "author": "mrozio13pl",
    "license": "MIT",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "pkgroll --minify",
        "dev": "pkgroll --watch"
    },
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./config": {
            "types": "./config.d.ts",
            "require": {
                "types": "./dist/config.d.cts",
                "default": "./dist/config.cjs"
            },
            "default": {
                "types": "./dist/config.d.ts",
                "default": "./dist/config.js"
            }
        },
        "./router": {
            "types": "./router.d.ts",
            "require": {
                "types": "./dist/router.d.cts",
                "default": "./dist/router.cjs"
            },
            "default": {
                "types": "./dist/router.d.ts",
                "default": "./dist/router.js"
            }
        }
    },
    "files": [
        "dist",
        "config.d.ts",
        "router.d.ts"
    ],
    "dependencies": {
        "@sawerjs/config": "workspace:*",
        "@sawerjs/types": "workspace:*",
        "trouter": "^4.0.0"
    },
    "devDependencies": {
        "@sawerjs/tsconfig": "workspace:*",
        "@types/node": "^22.10.1",
        "pkgroll": "^2.5.1",
        "typescript": "^5.7.2"
    }
}
